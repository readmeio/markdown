{
  "name": "@readme/variable",
  "description": "React component for Variables",
  "version": "0.0.0",
  "main": "index.jsx",
  "dependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.6.2",
    "react": "^16.4.2"
  },
  "scripts": {
    "lint": "eslint -f unix . --ext .jsx --ext .js",
    "inspect": "jsinspect",
    "prettier": "prettier --list-different \"./**/**.{js,jsx}\"",
    "pretest": "npm run lint && npm run inspect && npm run prettier",
    "test": "jest --coverage"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "jest": {
    "testURL": "http://localhost",
    "setupFiles": [
      "./lib/enzyme"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "author": "Dom Harrington <dom@harrington-mail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:readme/@readme/variable.git"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.55",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "enzyme": "^3.3.0",
    "enzyme-react-adapter-future": "^1.1.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "jest": "^23.4.2",
    "jsinspect": "^0.12.7",
    "prettier": "^1.14.0"
  }
}
