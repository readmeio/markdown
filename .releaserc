{
  "branches": [
    "main"
  ],
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "angular",
        "releaseRules": [
          {
            "type": "feature",
            "release": "minor"
          },
          {
            "type": "feat",
            "release": "minor"
          },
          {
            "type": "enhance",
            "release": "minor"
          },
          {
            "type": "refactor",
            "release": "minor"
          },
          {
            "type": "style",
            "release": "patch"
          },
          {
            "type": "chore",
            "scope": "deps",
            "release": "patch"
          },
          {
            "type": "chore",
            "scope": "deps-dev",
            "release": "patch"
          }
        ],
        "parserOpts": {
          "noteKeywords": [
            "BREAKING"
          ]
        }
      }
    ],
    [
      "@semantic-release/release-notes-generator",
      {
        "preset": "conventionalcommits",
        "presetConfig": {
          "types": [
            {
              "type": "feature",
              "section": "âœ¨ New & Improved",
              "hidden": false
            },
            {
              "type": "feat",
              "section": "âœ¨ New & Improved",
              "hidden": false
            },
            {
              "type": "enhance",
              "section": "âœ¨ New & Improved",
              "hidden": false
            },
            {
              "type": "refactor",
              "section": "âœ¨ New & Improved",
              "hidden": false
            },
            {
              "type": "style",
              "section": "âœ¨ New & Improved",
              "hidden": false
            },
            {
              "type": "fix",
              "section": "ðŸ›  Fixes & Updates",
              "hidden": false
            },
            {
              "type": "perf",
              "section": "ðŸ›  Fixes & Updates",
              "hidden": false
            },
            {
              "type": "chore",
              "section": "ðŸ›  Fixes & Updates",
              "hidden": false
            },
            {
              "type": "docs",
              "section": "ðŸ“˜ Tests & Docs",
              "hidden": false
            },
            {
              "type": "test",
              "section": "ðŸ“˜ Tests & Docs",
              "hidden": true
            },
            {
              "type": "build",
              "hidden": true
            },
            {
              "type": "ci",
              "hidden": true
            }
          ]
        },
        "parserOpts": {
          "noteKeywords": [
            "BREAKING"
          ]
        },
        "writerOpts": {
          "commitsSort": [
            "subject",
            "scope"
          ],
          "headerPartial": "## Version {{version}}"
        }
      }
    ],
    [
      "@semantic-release/changelog",
      {
        "changelogTitle": "Changelog\n==="
      }
    ],
    "@semantic-release/npm",
    [
      "@semantic-release/github",
      {
        "successComment": "<img align=\"right\" height=\"100\" src=\"https://user-images.githubusercontent.com/886627/87183626-529d9580-c29b-11ea-8c54-5ca58a1f4a77.png\" />\n\n## This ${issue.pull_request ? \"PR\" : \"issue\"} was ${issue.pull_request ? \"released\" : \"resolved\"}!\n\nðŸš€ Changes included in [<b>v${nextRelease.version}</b>](/readmeio/markdown/releases/tag/v${nextRelease.version})\n<br>"
      }
    ],
    [
      "@semantic-release/git",
      {
        "message": "build(release): ðŸš€ v${nextRelease.version} ðŸ¦‰\n\n${nextRelease.notes}\n<!--SKIP CI-->"
      }
    ]
  ]
}
